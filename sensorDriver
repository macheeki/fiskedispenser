from machine import Pin
import time

DISTANCE_PIN = Pin(26, Pin.IN)
LED_PIN = Pin(15, Pin.OUT, Pin.PULL_UP)
def getDistance():
    starttime = time.time_ns()
    while DISTANCE_PIN.value() == True:
        pass
    endtime = time.time_ns()
    diff_in_cm = (endtime - starttime) / 100
    return diff_in_cm

def getLED():
    return LED_PIN

